<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.tkpphr.android.common.demo">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.tkpphr.android.common.demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tkpphr.android.common.demo.AsyncImageListActivity"
                  android:label="AsyncImageTest">
        </activity>
        <activity android:name="com.tkpphr.android.common.demo.SoundTestActivity"
                  android:label="SoundTest">
        </activity>
    </application>
</manifest>
